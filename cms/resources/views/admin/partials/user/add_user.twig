{% extends 'admin/layout_admin.twig' %}
{% block main %}
<!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            Blank Page
                            <small>Subheading</small>
                        </h1>
                        <ol class="breadcrumb">
                            <li>
                                <i class="fa fa-dashboard"></i>  <a href="index.html">Dashboard</a>
                            </li>
                            <li class="active">
                                <i class="fa fa-file"></i> Blank Page
                            </li>
                        </ol>
                    </div>
                </div>
                <!-- /.row -->
<div class="col-xs-12">

<form method="post"  action="/manage/user">

  <div class="form-group">
    <label for="title">Last Name</label>
    <input type="text" class="col-xs-6 form-control" id="title" name="last-name">
  </div>
  <div class="form-group">
    <label for="title">First Name</label>
    <input type="text" class="col-xs-6 form-control" id="title" name="first-name" >
  </div>
  <div class="form-group">
    <label for="title">Email</label>
    <input type="email" class="col-xs-6 form-control" id="title" name="email">
  </div>
  <div class="form-group">
    <label for="title">Password</label>
    <input type="password" class="col-xs-6 form-control" id="title" name="password">
  </div>
  <div class="form-group">
    <label for="title">Role</label>
    <select name ="category" class="form-control">


      <option value="admin">Admin</option>
       <option value="subscriber">Subscriber</option>
      

    </select>
  </div>
 
 
  <button type="submit" class="btn btn-success" name="submit">Submit</button>
</form>


 </div>
